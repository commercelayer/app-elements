    "js-yaml": "^4.1.0",
    "msw": "^2.10.4",
    "react-hook-form": "^7.60.0",
    "react-select": "^5.10.2",
    "remark-gfm": "^4.0.1",
    "storybook": "^9.0.16",
    "typescript": "~5.7.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^3.25.76"
