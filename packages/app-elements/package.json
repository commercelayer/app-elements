{
  "name": "@commercelayer/app-elements",
  "version": "6.2.0",
  "type": "module",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js"
    },
    "./style.css": {
      "import": "./dist/style.css"
    },
    "./vendor.css": {
      "import": "./dist/vendor.css"
    },
    "./tailwind.global.css": {
      "require": "./dist/tailwind.global.css"
    }
  },
  "engines": {
    "node": ">=18",
    "pnpm": ">=7"
  },
  "scripts": {
    "build": "tsc && vite build && pnpm build:css-vendor && pnpm build:tailwind-cfg",
    "build:tailwind-cfg": "cp ./src/styles/global.css ./dist/tailwind.global.css",
    "build:css-vendor": "pnpm dlx @tailwindcss/cli -i ./src/styles/vendor.css -o ./dist/vendor.css --minify",
    "lint:fix": "pnpm biome check --write",
    "test": "vitest run",
    "test:watch": "vitest",
    "ts:check": "tsc --noEmit",
    "generate:schema.order_rules": "curl -s https://core.commercelayer.io/api/public/schemas/order_rules | pnpx json-schema-to-typescript > ./src/ui/forms/RuleEngine/schema.order_rules.ts",
    "generate:schema.price_rules": "curl -s https://core.commercelayer.io/api/public/schemas/price_rules | pnpx json-schema-to-typescript > ./src/ui/forms/RuleEngine/schema.price_rules.ts"
  },
  "dependencies": {
    "@commercelayer/js-auth": "^6.7.2",
    "@commercelayer/sdk": "6.46.0",
    "@date-fns/tz": "^1.4.1",
    "@monaco-editor/react": "4.7.0",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^19.1.13",
    "@types/react-datepicker": "^7.0.0",
    "@types/react-dom": "^19.1.9",
    "classnames": "^2.5.1",
    "i18next": "^25.5.2",
    "i18next-resources-to-backend": "^1.2.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "monaco-editor": "0.53.0",
    "pluralize": "^8.0.0",
    "query-string": "^9.3.0",
    "react": "^19.1.1",
    "react-currency-input-field": "^3.10.0",
    "react-datepicker": "^8.7.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.3",
    "react-select": "^5.10.2",
    "react-toastify": "^11.0.5",
    "react-tooltip": "^5.29.1",
    "swr": "^2.3.6",
    "ts-invariant": "^0.10.3",
    "type-fest": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@phosphor-icons/react": "v2.1.10",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.13",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.19.8",
    "@types/pluralize": "^0.0.33",
    "@types/react-gtm-module": "^2.0.4",
    "@vitejs/plugin-react": "^5.0.2",
    "cross-fetch": "^4.1.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.2",
    "postcss": "^8.5.6",
    "react-gtm-module": "^2.0.11",
    "tailwindcss": "^4.1.13",
    "typescript": "~5.9.2",
    "vite": "^7.1.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "wouter": "^3.7.1"
  },
  "peerDependencies": {
    "@commercelayer/sdk": "^6.x",
    "query-string": "^9.x",
    "react": "^19.1.x",
    "react-dom": "^19.1.x",
    "react-gtm-module": "^2.x",
    "react-hook-form": "^7.x",
    "wouter": "^3.x"
  }
}
